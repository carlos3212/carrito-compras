<h1>{{title}}</h1>
{{#if products }}
    <div class="cart page">
        <ul class="list-group">
            {{#each products }}
            <li class="list-group-item clearfix">
                <div class="col-xs-5">
                    <strong>{{ this.item.category_id }}</strong><br/>
                    <em>{{{this.item.prod_name}}}</em>
                </div>
                <div class="col-xs-2 text-right">
                    <span class="badge">{{ this.quantity }}</span>
                </div>
                <div class="col-xs-3 text-right">
                    {{ this.precio }}$
                </div>
                <div class="col-xs-2 text-right">
                    <a href="/remove/{{this.item._id}}" class="btn btn-default"></i> Eliminar</a>
                </div>
            </li>
            {{/each}}
        </ul>
    </div>
    <div class="text-right">
        <strong>Total: {{ totalPrice }} $</strong>
    </div>
    <hr>
    <div class="text-right">
        <a href="/" class="btn btn-default" type="button" class="btn btn-success">Checkout</a>
    </div>
{{else }}
    El carro esta limpio
{{/if}}
